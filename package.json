  "homepage": "https://github.com/ahsanghalib/react-vite-vitest-storybook-typescript",
  "engines": {
    "node": ">= 16.14.0"
  },
  "scripts": {
    "preinstall": "npx -y only-allow pnpm",
    "dev": "./react.sh start",
    "build": "./react.sh build",
    "preview": "vite preview",
    "lint": "eslint src",
    "format": "prettier src --check",
    "lint:fix": "eslint src --fix",
    "format:fix": "prettier src --check --write",
    "lint:format": "pnpm format:fix && pnpm lint:fix",
    "prepare": "husky install",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "start:sb": "./story.sh start",
    "build:sb": "./story.sh build",
    "sb:publish": "pnpm chromatic --project-token="
  },
  "dependencies": {
    "dayjs": "^1.11.7",
    "immer": "^9.0.18",
    "qs": "^6.11.0",
    "ramda": "^0.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.2",
    "react-router-dom": "^6.6.2",
    "styled-components": "^5.3.6",
    "whatwg-fetch": "^3.6.2",
    "zustand": "^4.3.2"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.5.15",
