    "vitest": "^0.27.1",
    "vitest-dom": "^0.0.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "pnpm run lint:fix",
      "pnpm run format:fix"
    ],
    "*.{html,css,js,json,md}": "pnpm run format:fix"
  }
}
