  "license": "MIT",
  "repository": "https://github.com/ahsanghalib/react-vite-vitest-storybook-typescript",
  "bugs": {
    "url": "https://github.com/ahsanghalib/react-vite-vitest-storybook-typescript/issues",
    "email": "ahsanghalib@gmail.com"
